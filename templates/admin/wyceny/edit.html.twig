        function copyValueToTarget(sourceElementId, targetElementId, targetSztuk, targetWartosc) {
            var sourceElement = document.getElementById(sourceElementId);
            var sourceValue = parseFloat(sourceElement.value.replace(',', '.'));
            var targetElement = document.getElementById(targetElementId);
            if (!isNaN(sourceValue)) {
                var targetValue = sourceValue;

                targetElement.value = targetValue.toFixed(2).replace('.', ',');
            }


            //ilość już z wazonu
            var sourceElementIloscW = document.getElementById(targetSztuk);
            var sourceValueIloscW = parseFloat(sourceElementIloscW.value.replace(',', '.'));

            //jeśli jest ilość w wazonie, to wprowadz nową wartość
            if (!isNaN(sourceValueIloscW)) {
                var newWartosc = sourceValueIloscW * sourceValue;

                var targetWartoscEl = document.getElementById(targetWartosc);
                targetWartoscEl.value = newWartosc.toFixed(2).replace('.', ',');

                //i przelicz sumę
                calculateSumWazon();
            }


        }

        function copyCustomNameToTarget(sourceElementId, targetElementId) {
            var sourceElement = document.getElementById(sourceElementId);
            var sourceValue = sourceElement.value;
            var targetElement = document.getElementById(targetElementId);
            targetElement.value = sourceValue;
        }


