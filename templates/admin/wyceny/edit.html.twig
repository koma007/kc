    <script>
        function calculateSumSolo() {
            var sum_koszty = 0;
            for (var i = 1; i <= 21; i++) {
                var inputId = 'Wyceny_wartosc' + i;
                var inputValue = $('#' + inputId).val();
                if (inputValue) {
                    var value = parseFloat(inputValue.replace('.', '').replace(',', '.'));
                    if (!isNaN(value)) {
                        sum_koszty += value;
                    }
                }
            }

            // koszt
            $('#solo_koszt').text(sum_koszty.toLocaleString('pl-PL') + ' zł');

            //marza pod kosztem
            var marza_solo = 0;
            var marza_solo_formatted=0;
            const blokadaSolo = $('#Wyceny_blokujMarzaSolo').prop('checked');

            var marza_solo =0;
            if (!blokadaSolo) {
                marza_solo = 0.3 * sum_koszty;
                marza_solo = marza_solo.toFixed(2).replace('.', ',');
                marza_solo_formatted = marza_solo.toLocaleString('pl-PL', {minimumFractionDigits: 2});
                $('#Wyceny_marza_solo').val(marza_solo_formatted);
            }else{
                marza_solo = $("#Wyceny_marza_solo").val(); // Pobranie wartości pola tekstowego
                marza_solo_formatted = marza_solo.toLocaleString('pl-PL', {minimumFractionDigits: 2});
                $('#Wyceny_marza_solo').val(marza_solo_formatted);
            }



            //solo - góra
            var total_solo = sum_koszty + parseFloat(marza_solo_formatted.replace(',', '.'));
            $('#solo_wartosc').text(total_solo.toLocaleString('pl-PL') + ' zł');

            //solo -dół
            var total_solo = total_solo.toLocaleString('pl-PL', { minimumFractionDigits: 2 });
            $('#Wyceny_cena_solo').val(total_solo);

            //kpl - góra
            setTimeout(function() {
                var wartoscSolo = parseFloat($('#solo_wartosc').text().replace(',', '.'));
                var wazonWartosc = parseFloat($('#wazon_wartosc').text().replace(',', '.'));

                var kpl_total = (wartoscSolo + wazonWartosc).toFixed(2);
                kpl_total = kpl_total.replace('.', ',');
                $('#kpl_wartosc').text(kpl_total.toLocaleString('pl-PL') + ' zł');
                $('#kpl_tytul, #kpl_wartosc').css({
                    "background-color": "#C9E4C9",
                    "font-weight": "bold"
                });

                var kpl_total_2w = (wartoscSolo + (wazonWartosc*2)).toFixed(2);
                kpl_total_2w = kpl_total_2w.replace('.', ',');
                $('#kpl_2w_wartosc').text(kpl_total_2w.toLocaleString('pl-PL') + ' zł');

                //zapis
                $('#Wyceny_cena_kpl').val(kpl_total);
                $('.zielony-prawa, .zielony-prawa input, span.zielony').css({
                    "background-color": "#C9E4C9",
                    "font-weight": "bold",
                });

                $('#Wyceny_cena_kpl_2w').val(kpl_total_2w);

            }, 50);
        }

