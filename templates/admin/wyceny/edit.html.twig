            if (where >= 8 && where <= 18) {
                whereWazon = where + 21;

                cenaFieldIdWazon = `Wyceny_cena${whereWazon}`;
                sztukFieldIdWazon = `Wyceny_sztuk${whereWazon}`;
                wartoscFieldIdWazon = `Wyceny_wartosc${whereWazon}`;
                skladowaFieldIdWazon = `Wyceny_skladowa${whereWazon}_autocomplete-ts-control .items-placeholder`;
            }

            $.ajax({
                url: specificPriceUrl,
                type: 'POST',
                success: (data) => {

                    $('#' + cenaFieldId).val(data.cena);

                    var sztukValue = ($('#' + sztukFieldId).val().replace(',', '.')); // Parsowanie wartości do liczby zmiennoprzecinkowej
                    var cena = parseFloat(data.cena);
                    var newValue = (cena * sztukValue).toFixed(2);
                    $('#' + wartoscFieldId).val(newValue);



                    //wazon
                    if (where >= 8 && where <= 18) {
                        $('#' + cenaFieldIdWazon).val(data.cena);
                        $('#' + sztukFieldIdWazon).val(1);
                        $('#' + wartoscFieldIdWazon).val(data.cena);
                        $('#' + skladowaFieldIdWazon).attr('placeholder', data.nazwa);
                    }




                    calculateSumSolo();
                    calculateSumWazon();
                }
            });
        };


        function calculateWartosc(value, where) {
            const sztuk = document.getElementById('Wyceny_sztuk' + where).value.replace(',', '.');
            const cena = document.getElementById('Wyceny_cena' + where).value.replace(',', '.');
            const wartosc = (sztuk * cena).toFixed(2);

            document.getElementById('Wyceny_wartosc' + where).value = wartosc.toString().replace(/\./g, ',');
            calculateSumSolo();
            calculateSumWazon();
        }
    </script>
{% endblock %}

